# Cao Backend - Environment Variables

# App
APP_ENV=development
DEBUG=true
API_VERSION=1.0.0

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:3000

# Frontend URL (used for share URLs etc.)
FRONTEND_URL=http://localhost:3000

# Supabase (for storage access - optional for MVP)
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_KEY=eyJ...

# Rate Limiting
RATE_LIMIT_PER_MINUTE_UNAUTHENTICATED=10
RATE_LIMIT_PER_MINUTE_AUTHENTICATED=30

# Image Processing
MAX_IMAGE_SIZE_MB=10
MAX_IMAGE_DIMENSION=2048

# Replicate API (Face Swap)
REPLICATE_API_TOKEN=r8_xxxxx
SWAP_CACHE_TTL=3600
SWAP_CACHE_MAX_SIZE=100
